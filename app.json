{
  "expo": {
    "name": "Museeach",
    "description": "Each mobile app",
    "slug": "each-react-native-app",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "expo.each",
    "icon": "./assets/icons/each_icon.png",
    "loading": {
      "icon": "./assets/icons/loading_icon.png"
    },
    "notification": {
      "icon": "./assets/icons/notification-icon.png",
      "color": "#000000"
    },

    "packagerOpts": {
      "assetExts": ["ttf", "mp4"]
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ITS.each"
    },
    "android": {
      "versionCode": 3,
      "config":{
        "googleMaps": {
          "apiKey": "AIzaSyDU2_RvB1pkPqXsiqg2YIZPZbnEAhrn4sQ"
        }
      },
      "package": "com.ITS.each",
      "permissions": [
        "CAMERA",
        "MANAGE_DOCUMENTS",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    }
  }
}
